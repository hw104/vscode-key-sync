{
	"name": "key-sync",
	"displayName": "Key Sync",
	"description": "Tiny VSCode Extension Synchronize **only** keybinding configuration.",
	"version": "0.0.1",
	"publisher": "hw104",
	"engines": {
		"vscode": "^1.59.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:key-sync.upload",
		"onCommand:key-sync.uploadForce",
		"onCommand:key-sync.update",
		"onCommand:key-sync.updateForce"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "key-sync.upload",
				"title": "Key Sync: Upload"
			},
			{
				"command": "key-sync.uploadForce",
				"title": "Key Sync: Upload (Force)"
			},
			{
				"command": "key-sync.update",
				"title": "Key Sync: Update"
			},
			{
				"command": "key-sync.updateForce",
				"title": "Key Sync: Update (Force)"
			}
		],
		"configuration": {
			"title": "Key Sync",
			"properties": {
				"key-sync.GitRemoteRepository": {
					"type": [
						"string"
					],
					"description": "Specifies an existing git remote repository"
				},
				"key-sync.GitBranch": {
					"type": [
						"string"
					],
					"description": "Specifies an existing git branch"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.59.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vsce": "^1.96.1",
		"vscode-test": "^1.5.2"
	}
}
